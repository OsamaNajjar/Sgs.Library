@model List<BookViewModel>
@{
    ViewData["Title"] = "Books";
}

<div class="row fadeIn" style="padding-top:40px;margin:30px 30px 15px 30px">
    <div class="col-md-5 offset-md-1">
        <i class="fas fa-book" style="font-size: 60px; color: #5eaaa8;"><span style="font-size:24pt;color:#5eaaa8">  Books</span></i>
    </div>

</div>

@foreach (var books in Model)
{
<h1>@books.Code</h1>
}